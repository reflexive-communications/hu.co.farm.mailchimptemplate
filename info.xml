<?xml version="1.0"?>
<extension key="hu.co.farm.mailchimptemplate" type="module">
    <file>mailchimptemplate</file>
    <name>Mailchimp Template Import</name>
    <description>Imports Mailchimp templates</description>
    <license>AGPL-3.0</license>
    <authors>
        <author>
            <name>Sandor Semsey</name>
            <email>sandor@es-progress.hu</email>
            <role>Maintainer</role>
        </author>
    </authors>
    <urls>
        <url desc="Main Extension Page">https://github.com/reflexive-communications/hu.co.farm.mailchimptemplate</url>
        <url desc="Documentation">https://github.com/reflexive-communications/hu.co.farm.mailchimptemplate</url>
        <url desc="Support">https://github.com/reflexive-communications/hu.co.farm.mailchimptemplate/issues</url>
        <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
    </urls>
    <releaseDate>2023-04-07</releaseDate>
    <version>2.0.0</version>
    <develStage>stable</develStage>
    <compatibility>
        <ver>5.47</ver>
    </compatibility>
    <requires>
        <ext>rc-base</ext>
    </requires>
    <classloader>
        <psr0 prefix="CRM_" path="."/>
        <psr4 prefix="Civi\" path="Civi"/>
    </classloader>
    <mixins>
        <mixin>menu-xml@1.0.0</mixin>
    </mixins>
    <civix>
        <namespace>CRM/Mailchimptemplate</namespace>
        <format>22.12.1</format>
    </civix>
</extension>
